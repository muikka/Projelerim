
@model MvcLearning.Models.Customer

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li><strong>Membership Type:</strong> @Model.MembershipType.Name</li>

    @if (Model.Birthdate.HasValue)
    {
        <li><strong>Birthdate:</strong>  @Model.Birthdate.Value.ToShortDateString()</li>
    }
    
    
</ul>

